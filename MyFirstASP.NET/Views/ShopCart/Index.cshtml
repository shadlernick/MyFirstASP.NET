@model ShopCartViewModel
<div class="container">
    @foreach (var el in Model.ShopCart.shopCartItems)
    {
        <div class="alert alert-warning mt-3">
            <b>Car:</b> @el.Car.Name<br/>
            <b>Price:</b> @el.Car.Price.ToString("c")
        </div>
    }
    <hr/>
    <div class="btn btn-danger" asp-controller="Order">Pay</div>
</div>
